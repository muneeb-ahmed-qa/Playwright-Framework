{
  "name": "playwright-automation-framework",
  "version": "1.0.0",
  "description": "A comprehensive Playwright automation framework showcasing advanced testing capabilities and best practices",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome'",
    "test:staging": "playwright test --project=staging",
    "test:production": "playwright test --project=production",
    "test:parallel": "playwright test --workers=4",
    "test:serial": "playwright test --workers=1",
    "test:retry": "playwright test --retries=2",
    "test:update-snapshots": "playwright test --update-snapshots",
    "test:codegen": "playwright codegen",
    "test:show-report": "playwright show-report",
    "test:show-trace": "playwright show-trace",
    "install:browsers": "playwright install",
    "install:deps": "playwright install --with-deps",
    "lint": "eslint tests/ pages/ utils/",
    "lint:fix": "eslint tests/ pages/ utils/ --fix",
    "format": "prettier --write tests/ pages/ utils/",
    "format:check": "prettier --check tests/ pages/ utils/",
    "clean": "rimraf test-results/ playwright-report/ screenshots/ downloads/",
    "setup": "npm install && npm run install:deps",
    "start": "echo 'Please update the baseURL in playwright.config.js with your application URL'",
    "docs": "echo 'Documentation available in docs/ directory'",
    "examples": "echo 'Example tests available in tests/ directory'",
    "encrypt": "node scripts/encryptPassword.js",
    "generate-password": "node scripts/encryptPassword.js generate 16 --uppercase --lowercase --numbers --symbols",
    "test:encryption": "npx playwright test tests/passwordEncryption.spec.js",
    "setup:credentials": "node scripts/setupEncryptedCredentials.js",
    "test:login": "npx playwright test tests/login.spec.js",
    "test:gmail": "npx playwright test tests/gmail.spec.js",
    "test:performance": "npx playwright test tests/performance.spec.js",
    "test:performance:headed": "npx playwright test tests/performance.spec.js --headed",
    "test:data-management": "npx playwright test tests/testDataManagement.spec.js",
    "setup:google-api": "node scripts/setupGoogleApi.js",
    "data:generate": "node scripts/testDataCli.js generate",
    "data:cleanup": "node scripts/testDataCli.js cleanup",
    "data:validate": "node scripts/testDataCli.js validate",
    "data:export": "node scripts/testDataCli.js export",
    "data:import": "node scripts/testDataCli.js import",
    "data:stats": "node scripts/testDataCli.js stats",
    "data:help": "node scripts/testDataCli.js help",
    "test:parallel": "npx playwright test --workers=4",
    "test:smoke": "npx playwright test tests/smoke.spec.js",
    "test:integration": "npx playwright test tests/integration.spec.js",
    "deploy:staging": "./scripts/deploy.sh deploy staging",
    "deploy:production": "./scripts/deploy.sh deploy production",
    "deploy:rollback": "./scripts/deploy.sh rollback",
    "docker:build": "docker build -t playwright-test-framework .",
    "docker:run": "docker run -p 9323:9323 playwright-test-framework",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "helm:install": "helm install playwright-framework ./helm/playwright-framework",
    "helm:upgrade": "helm upgrade playwright-framework ./helm/playwright-framework",
    "helm:uninstall": "helm uninstall playwright-framework",
    "setup:google-api:url": "node scripts/setupGoogleApi.js --generate-url",
    "setup:google-api:exchange": "node scripts/setupGoogleApi.js --exchange-code"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "e2e",
    "end-to-end",
    "web-automation",
    "page-object-model",
    "api-testing",
    "cross-browser",
    "mobile-testing",
    "ci-cd",
    "framework",
    "showcase",
    "best-practices"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "googleapis": "^128.0.0",
    "@faker-js/faker": "^8.4.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/playwright-automation-framework.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/playwright-automation-framework/issues"
  },
  "homepage": "https://github.com/your-username/playwright-automation-framework#readme",
  "files": [
    "pages/",
    "tests/",
    "utils/",
    "fixtures/",
    "docs/",
    "playwright.config.js",
    "README.md"
  ],
  "playwright": {
    "version": "1.40.0"
  }
}
